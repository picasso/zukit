{
	"name": "zukit",
	"version": "2.0.1",
	"testedWP": "6.6.2",
	"requiresPHP": ">=7.2.0",
	"type": "module",
	"repository": {
		"type": "git",
		"url": "https://github.com/picasso/zukit.git"
	},
	"keywords": [
		"wordpress",
		"framework",
		"plugin",
		"gutenberg",
		"responsive"
	],
	"author": "Dmitry Rudakov",
	"description": "Implements basic functionality for managing scripts and creating plugin or theme settings page based on Gutenberg functionality.",
	"license": "MIT",
	"scripts": {
		"dev": "wp-scripts start --config wp-dev.config.cjs",
		"build": "wp-scripts build --config wp-scripts.config.cjs",
		"---------": "",
		"lint:eslint": "eslint src/scripts",
		"lint:fix": "npm run lint:eslint -- --fix",
		"format:check": "prettier --check \"src/**/*.{ts,tsx,js,jsx,json,scss}\"",
		"format": "prettier --write \"src/**/*.{ts,tsx,js,jsx,json,scss}\"",
		"----------": "",
		"plugin-zip": "wp-scripts plugin-zip",
		"packages-update": "wp-scripts packages-update"
	},
	"dependencies": {
		"clsx": "^2.1.1",
		"lodash-es": "^4.17.21"
	},
	"devDependencies": {
		"@eslint/js": "^9.17.0",
		"@wordpress/eslint-plugin": "^22.1.1",
		"@wordpress/scripts": "^30.8.1",
		"browser-sync-webpack-plugin": "^2.3.0",
		"eslint": "^9.17.0",
		"eslint-plugin-react": "^7.37.3",
		"eslint-plugin-react-hooks": "^5.1.0",
		"eslint-plugin-simple-import-sort": "^12.1.1",
		"globals": "^15.14.0",
		"prettier": "^3.4.2",
		"replace-in-file": "^8.3.0",
		"typescript": "^5.7.2"
	}
}
